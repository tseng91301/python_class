        $('body').on('change', '#pass', function() {
          if (this.checked === true) {
            $('#link_pass').attr("disabled", false);
          } else {
            $('#link_pass').attr("disabled", true);
          }
        
        })
        $('body').on('change', '#enableemail', function() {
          if (this.checked === true) {
            $('#link_rcpt').attr("disabled", false);
          } else {
            $('#link_rcpt').attr("disabled", true);
          }
        })
            </script>
